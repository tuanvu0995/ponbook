<div>
  <h2 class="title is-5">{{ title }}</h2>
  @if(subtitle)
  <p class="subtitle is-6">
    {{ subtitle }}
  </p>
  @endif
  <div class="columns is-multiline is-mobile is-tablet is-desktop is-widescreen listings__container
    @if (hideLastChild)
    is-hidden-last-child
    @endif
  ">
    @each(video in videos)
      <div class="column is-6-mobile-xs is-4-mobile-md is-3-tablet is-2-desktop">
        @!component('components/listing', { video })
      </div>
    @endeach
  </div>
</div>
