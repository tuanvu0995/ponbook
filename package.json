{
  "name": "ponbook",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "dockerAttach": "docker exec -it ponbook /bin/sh",
    "dockerAttachRedis": "docker exec -it redis /bin/sh",
    "dockerAttachPostgres": "docker exec -it postgres /bin/sh"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.18.6",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.2.2",
    "@symfony/webpack-encore": "4.1.1",
    "@types/lodash": "^4.14.192",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.6",
    "adonis-preset-ts": "^2.1.0",
    "awesome-image-viewer": "^1.0.60",
    "bulma": "^0.9.4",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.0.0",
    "jquery": "^3.6.3",
    "openapi-types": ">=7",
    "owl.carousel": "^2.3.4",
    "pino-pretty": "^9.1.1",
    "postcss": ">=8.3.0 <9.0.0",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "sticky-sidebar-v2": "^1.1.1",
    "toastify-js": "^1.12.0",
    "typescript": "~4.6",
    "webpack": "^5.72",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/bouncer": "^2.3.0",
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/drive-s3": "^1.3.2",
    "@adonisjs/i18n": "^1.5.6",
    "@adonisjs/limiter": "^1.0.1",
