{
  "name": "ponbook",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=16.x.x"
  },
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@adonisjs/eslint-plugin": "^1.1.8",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.2.2",
    "@types/lodash": "^4.14.192",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "pino-pretty": "^9.1.1",
    "prettier": "2.8.8",
    "typescript": "~4.6"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/bouncer": "^2.3.0",
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/drive-s3": "^1.3.2",
    "@adonisjs/limiter": "^1.0.1",
    "@adonisjs/lucid": "^18.3.0",
    "@adonisjs/mail": "^8.1.2",
    "@adonisjs/redis": "^7.3.2",
    "@adonisjs/repl": "^3.1.0",
    "@adonisjs/route-model-binding": "^1.0.1",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.1.0",
    "@adonisjs/view": "^6.1.0",
    "@melchyore/adonis-cache": "^1.0.6",
    "adonis-lucid-soft-deletes": "^1.4.4",
    "adonis-turnstile": "^1.0.4",
    "adonis5-sentry": "^3.1.0",
    "axios": "^1.2.1",
    "limax": "^3.0.0",
    "luxon": "^3.1.1",
    "mysql2": "^2.3.3",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "uniqid": "^5.4.0",
    "youch": "^3.2.2",
    "youch-terminal": "^2.1.5"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "main": "index.js",
  "repository": "git@github.com:tuanvu0995/ponbook.git",
  "author": "tuanvu0995 <tuanvu0995@gmail.com>",
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  }
}
